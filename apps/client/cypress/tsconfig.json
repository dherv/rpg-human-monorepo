{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "jsx": "react-jsx",
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "types": ["cypress", "node", "@testing-library/cypress"]
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
