{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "dev": {},
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typescript": {
      "dependsOn": ["^typescript"]
    },
    "preview": {
      "dependsOn": ["^preview"]
    },
    "cypress": {
      "dependsOn": ["^cypress"]
    }
  }
}
